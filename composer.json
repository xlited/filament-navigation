{
    "name": "xlited/filament-navigation",
    "description": "Build structured navigation menus in Filament.",
    "keywords": [
        "laravel",
        "filament",
        "navigation"
    ],
    "homepage": "https://github.com/xlited/filament-navigation",
    "license": "MIT",
    "authors": [
        {
            "name": "Xlite Dev",
            "email": "hello@xlite.dev"
        },
        {
            "name": "Van Ons",
            "email": "opensource@van-ons.nl",
            "role": "Developer"
        },
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87",
        "larastan/larastan": "^3.6",
        "nunomaduro/collision": "^8.8",
        "orchestra/canvas": "^9.0|^10.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ray": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "VanOns\\FilamentNavigation\\": "src",
            "VanOns\\FilamentNavigation\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VanOns\\FilamentNavigation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes",
        "format:check": "vendor/bin/php-cs-fixer fix --dry-run --config=.php_cs.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VanOns\\FilamentNavigation\\FilamentNavigationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
